Blockly.Blocks['input'] = {
  init: function() {
    this.appendValueInput("in")
        .setCheck(null)
        .appendField("input ");
    this.setInputsInline(true);
    this.setOutput(true, "String");
    this.setColour(160);
 this.setTooltip("verkrijg gebruikersinput van de console. (optioneel: geef een waarde om weer te geven in de console (bijvoorbeeld als vraag))");
 this.setHelpUrl("");
  }
};